input {
  tcp {
    port => 9600
    codec => json_lines
  }
}
filter {
  mutate {
    add_field => { "log_processed_by" => "logstash" }
  }
}
output {
  elasticsearch {
    hosts => ["http://{{ elasticsearch_host }}:9200"]
    index => "microservices-%{+YYYY.MM.dd}"
  }
  stdout { codec => rubydebug }
}
